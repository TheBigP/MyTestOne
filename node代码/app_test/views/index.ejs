<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <!-- <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.min.css"> -->
    <link href="http://cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
  </head>
  <body>
    <form class="form-horizontal" method="post" role="form">
      <div class="control-group">
        <label class="control-label" for="inputEmail">User</label>
        <div class="controls">
          <input type="text" id="inputEmail" placeholder="Email" name="admin_id">
        </div>
      </div>
      <div class="control-group">
        <label class="control-label" for="inputPassword">Password</label>
        <div class="controls">
          <input type="password" id="inputPassword" placeholder="Password" name="admin_psd">
        </div>
      </div>
      <div class="control-group">
        <div class="controls">
          <label class="checkbox">
            <input type="checkbox"> Remember me
          </label>
          <button type="submit" class="btn">Sign in</button>
        </div>
      </div>
    </form>
    <% if (locals.error){  %>
      <p><%= error %></p><br>
    <% } %>
    <script src="http://libs.baidu.com/jquery/2.1.3/jquery.min.js"></script>

    <script src="/lib/bootstrap/dist/js/bootstrap.min.js" type="text/javascript" charset="utf-8"></script>

<!-- <script src="http://cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap.min.js"></script> -->
<!-- <script src="http://cdn.bootcss.com/jquery/3.0.0/jquery.min.js"></script> -->


  </body>
</html>
